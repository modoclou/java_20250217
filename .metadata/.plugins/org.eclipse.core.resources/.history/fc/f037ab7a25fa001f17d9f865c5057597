package com.company.java013;

class A11{
	int a; //(1)
	A11(){}
	A11(int a){ this.a=a;} //(2)
	//(3) void show()
	void show() { this.a=11; System.out.println(this.a); }
	//(4) static void classMethod()
	static void classMethod() {this.a=12;}
	//(5) int show2()
	int show2() {int a; return a;}
	}
}


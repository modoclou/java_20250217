package com.company.java012;

//1. 클래스는 부품객체
//2. 상태(멤버변수)와 행위(멤버함수)
//3. 상속 - 클래스의 재사용(재활용)

/*
	Object
	  ↑
	MobileNote7   String iris(private) / void newshow();
	  ↑
	MobileNote8   String face(private) / void newshow();
	  ↑
	MobileNote9   int battery(private) / void newshow();
 */

class MobileNote7 extends Object{
	private String iris;
	public String getIris() { return iris; }
	public void setIris(String iris) { this.iris = iris; }
	
	public void newshow() {	System.out.print(":::: NOTE7 = 홍채인식 > " + this.iris); }	
}
class MobileNote8 extends MobileNote7{
	private String face;	
	public String getFace() { return face; }
	public void setFace(String face) { this.face = face; }
	
	public void newshow() {	System.out.print(":::: NOTE8 = 안면인식 > " + this.face); }	
}
class MobileNote9 extends MobileNote7{
	private int battery=24;
	public int getBattery() { return battery; }
	public void setBattery(int battery) { this.battery = battery; }
//	public void setIris(String string) { }
	public void setFace(String string) { }
	
	public void newshow() {	System.out.print(":::: NOTE9 = 하루 종일 배터리 사용 가능 > " + this.battery); }	
}


public class Extends003 {
	public static void main(String[] args) {
		MobileNote9 my9 = new MobileNote9();
		my9.setIris("brown");
		my9.setFace("pretty");
		my9.newshow();
	}
}

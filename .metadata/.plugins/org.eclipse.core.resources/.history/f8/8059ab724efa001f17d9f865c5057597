package com.company.java013;

/**
 □ 사각형의 넓이 : 100 
 □ 삼각형의 넓이 : 50 
 □ 원의 넓이 : 314.1592653589793 
 **/

abstract class Shape002{
	showArea(int w, int h);
	showArea(int r);
}
abstract class Rectangle002{}
abstract class Triangle002{}
abstract class Circle002{}

public class Abstract002 {
	public static void main(String[] args) {
		Rectangle002 rec = new Rectangle002();
		rec.showArea(10, 10);					///가로*세로
		Triangle002 tri= new Triangle002();
		tri.showArea(10, 10);					///(가로*세로)/2
		Circle002 cir = new Circle002();
		cir.showArea(10);						///반지름*반지름*ㅠ값
	}
}

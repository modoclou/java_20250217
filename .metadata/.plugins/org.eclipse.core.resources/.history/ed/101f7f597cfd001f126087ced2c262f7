package com.company.java015_ex;

public class Coffee {
	public Coffee() { super(); }
	public Coffee(Coffee a1, Coffee a2) { super(); this.a1 = a1; this.a2 = a2; }
	@Override public String toString() { return "Coffee [a1=" + a1 + ", a2=" + a2 + "]"; }
	
	public Coffee getA1() { return a1; }
	public void setA1(Coffee a1) { this.a1 = a1; }
	public Coffee getA2() { return a2; }
	public void setA2(Coffee a2) { this.a2 = a2; }
}

public class Coffee012{
	public static void main(String[] args) {		
		Coffee a1 = new Coffee("까페라떼", 2, 4000);
		a1.show();
		Coffee a2 = new Coffee();
		a2.show();
	}
}
package com.company.java014;

interface 	Inter1{void method();} //public abstract
class 		Inter1Imp1 implements Inter1{
	@Override public void method() {System.out.println("...done!");}
//	@Override public String toString() { return "Inter1Imp1 []"; }
}// 1-1 클래스 구현

public class NonameClass001 {
	public static void main(String[] args) {
		//1. 인터페이스 상속 구현객체
		//1-2. 실체화객체 테스트
		//부모	  =     자식				@Override 자식메서드
		Inter1 i1 = new Inter1Imp1();	i1.method();	//...done!
		
		//2. 익명이너클래스
		Inter1 i2 = new Inter1Imp1(); //안되는 이유: {} 구현부가 없어서
	}
}

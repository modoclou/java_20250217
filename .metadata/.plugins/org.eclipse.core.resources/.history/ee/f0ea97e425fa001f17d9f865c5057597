package com.company.java013;

class A11{
	int a; //(1) 인스턴스 변수
	A11(){}
	A11(int a){ this.a=a; } //(2) - 지역변수
	//(3) void show() - 클래스 메서드
	void show() { this.a=11; System.out.println(this.a); }
	//(4) static void classMethod() - 클래스 변수
	static void classMethod() { this.a=12; }
	//(5) int show2() - 인스턴스 메서드
	int show2() {int a; return a;}
	}

public class RepeatQ123 {
	public static void main(String[] args) {
		A11 a1 = new A11(); (b)
	}
} //end class